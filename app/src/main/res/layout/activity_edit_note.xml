<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.eseo.dis.couroulu.eseo_app_project.MyJuriesFragment">


    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">

        <ListView android:id="@+id/listView" android:layout_width="wrap_content" android:layout_height="wrap_content" />

        <Button
            android:id="@+id/buttonUpdateNoteStudent"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/listView"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:text="@string/txtUpdateButtonNote"/>
    </RelativeLayout>



</FrameLayout>
